{#
/**
 * @file
 * default theme for bloc followed me
 *
 * Use to override for block.html.twig
 */
#}
<div{{ attributes }}>
    {{ title_prefix }}
    {% if label %}
        <h2{{ title_attributes }}>{{ label }}</h2>
    {% endif %}
    {{ title_suffix }}

    <div{{ content_attributes }}>
        {% block content %}
                {{ content }}
                {{ dump() }}
        {% endblock %}
    </div>
</div>